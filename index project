        <button onclick="myfunction()">CLICK</button>
<script>
    function myfunction(){
        console.log("function executed");
        var num1=document.getElementById("num1").value;
        var select=[];
         select=document.getElementsByClassName("option");
         console.log(select);
        for(var i=0;i<select.length;i++){
            var compare=select[i].innerHTML;
            select[i]==compare[1];
            if(compare.substring(0,select.length)==num1){
                console.log("executed");
            }else{
                select[i].innerHTML="";
            }
        }

    }
</script>
    
